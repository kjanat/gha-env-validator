{
  "entryFileName": "index.md",
  "entryPoints": [
    "src/index.ts"
  ],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "fileExtension": ".md",
  "hideGenerator": true,
  "navigation": {
    "includeCategories": true,
    "includeFolders": false
  },
  "out": "docs/api",
  "outputFileStrategy": "modules",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "readme": "none",
  "skipErrorChecking": true
}
